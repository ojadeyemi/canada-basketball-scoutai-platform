# Root .env.local for Docker Compose
# Copy this to .env.local and fill in your actual values

# Database (only for local non-Docker development - Docker uses docker-compose config)
# DATABASE_URL=postgresql+psycopg://YOUR_USERNAME:YOUR_PASSWORD@localhost:5432/canada_basketball_ai

# Google Cloud Storage
GCS_BUCKET_NAME=canada-basketball-scouting-reports

# LLM API Keys
COHERE_API_KEY=your_cohere_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Model Configuration (optional - leave empty to use DEFAULT_MODEL)
DEFAULT_MODEL=command-a-03-2025
ROUTER_MODEL=
SQL_MODEL=
SCOUTING_MODEL=

# Application Security
API_KEY=your_api_key_here

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
VITE_BACKEND_URL=http://localhost:8080/api